{
  "name": "hermione",
  "version": "6.0.3",
  "description": "Tests framework based on mocha and wdio",
  "main": "src/lib/hermione.js",
  "types": "./typings/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc",
    "coverage": "nyc --reporter=text npm run test-unit",
    "clean": "rimraf build/ *.tsbuildinfo",
    "prepack": "npm run clean && npm run build",
    "lint": "eslint .",
    "reformat": "npm run lint -- --fix",
    "test-unit": "_mocha src/test/ --config src/test/.mocharc.json",
    "test": "npm run test-unit && npm run lint",
    "toc": "doctoc README.md",
    "precommit": "npm run lint",
    "preversion": "npm test",
    "commitmsg": "commitlint -e",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/gemini-testing/hermione.git"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "keywords": [
    "hermione",
    "webdriverio",
    "mocha",
    "test"
  ],
  "bin": {
    "hermione": "./bin/hermione"
  },
  "license": "MIT",
  "dependencies": {
    "@gemini-testing/commander": "2.15.3",
    "@gemini-testing/mocha": "^9.3.0",
    "@types/mocha": "^9.0.0",
    "@wdio/types": "^7.26.0",
    "@wdio/utils": "^7.26.0",
    "aliasify": "^1.9.0",
    "bluebird": "^3.5.1",
    "browserify": "^13.3.0",
    "chalk": "^1.1.1",
    "clear-require": "^1.0.1",
    "debug": "^2.6.9",
    "expect-webdriverio": "^3.5.3",
    "fastq": "^1.13.0",
    "fs-extra": "^5.0.0",
    "gemini-configparser": "^1.3.0",
    "glob-extra": "^5.0.2",
    "inherit": "^2.2.2",
    "lodash": "^4.17.21",
    "looks-same": "^8.1.0",
    "micromatch": "^4.0.5",
    "plugins-loader": "^1.1.0",
    "png-validator": "1.1.0",
    "sharp": "~0.30.7",
    "sizzle": "^2.3.6",
    "temp": "^0.8.3",
    "uglifyify": "^3.0.4",
    "urijs": "^1.19.11",
    "url-join": "^4.0.1",
    "webdriverio": "7.28.0",
    "worker-farm": "^1.7.0",
    "yallist": "^3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^14.18.36",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "app-module-path": "^2.2.0",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "doctoc": "^2.2.0",
    "escape-string-regexp": "1.0.5",
    "eslint": "^8.25.0",
    "eslint-config-gemini-testing": "^2.8.0",
    "husky": "^0.11.4",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "nyc": "^15.1.0",
    "proxyquire": "^1.7.3",
    "rimraf": "^4.1.2",
    "sinon": "^4.4.2",
    "sinon-chai": "^2.12.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
